<?xml version="1.0" encoding="UTF-8"?>
<gestemas>
  <movementThreshold>1.0</movementThreshold>
  <Target>
    <name>root</name>
    <GestureRecognizers>
      <LongPress>
        <numTouchesRequired>1</numTouchesRequired>
        <minPressDuration>950</minPressDuration>
        <recognitionThresholdFactor>1.0</recognitionThresholdFactor>
        <allowSimultaneousRecognition>false</allowSimultaneousRecognition>
        <RightClick>
        </RightClick>
      </LongPress>
      <Pan>
        <maxNumTouchesRequired>1</maxNumTouchesRequired>
        <minNumTouchesRequired>1</minNumTouchesRequired>
        <recognitionThresholdFactor>1.0</recognitionThresholdFactor>
        <allowSimultaneousRecognition>false</allowSimultaneousRecognition>
        <Drag>
        </Drag>
      </Pan>
      <Tap>
        <numTouchesRequired>1</numTouchesRequired>
        <numTapsRequired>1</numTapsRequired>
        <maxTapDelay>400</maxTapDelay>
        <maxTapDuration>900</maxTapDuration>
        <maxTapDistance>100.0</maxTapDistance>
        <recognitionThresholdFactor>1.0</recognitionThresholdFactor>
        <allowSimultaneousRecognition>false</allowSimultaneousRecognition>
        <LeftClick>
        </LeftClick>
      </Tap>
      <Pan>
        <maxNumTouchesRequired>2</maxNumTouchesRequired>
        <minNumTouchesRequired>2</minNumTouchesRequired>
        <recognitionThresholdFactor>1.0</recognitionThresholdFactor>
        <allowSimultaneousRecognition>true</allowSimultaneousRecognition>
        <Scroll>
          <accumulator>2</accumulator>
          <maxVelocity>20.0</maxVelocity>
          <minVelocity>1.0</minVelocity>
        </Scroll>
      </Pan>
      <TwoTouchPinch>
        <recognitionThresholdFactor>2.0</recognitionThresholdFactor>
        <allowSimultaneousRecognition>true</allowSimultaneousRecognition>
        <Zoom>
          <accumulator>2</accumulator>
          <maxScale>1.50</maxScale>
          <minScale>1.01</minScale>
        </Zoom>
      </TwoTouchPinch>
    </GestureRecognizers>
  </Target>
</gestemas>
