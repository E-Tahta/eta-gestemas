<?xml version="1.0" encoding="UTF-8"?>
<gestemas>
  <GlobalTarget>
    <name>root</name>
    <GestureRecognizers>
      <TwoTouchPinch id="5">
        <recognitionThreshold>1.0</recognitionThreshold>
        <abortList>
          <allowSimultaneousRecognition>true</allowSimultaneousRecognition>
        </abortList>
        <XTestTwoTouchPinchZoom>
          <accumulator>8</accumulator>
          <maxScale>1.60</maxScale>
          <minScale>1.0001</minScale>
        </XTestTwoTouchPinchZoom>
      </TwoTouchPinch>
    </GestureRecognizers>
  </GlobalTarget>
</gestemas>
