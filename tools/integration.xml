<?xml version="1.0" encoding="UTF-8"?>
<gestemas>
  <samplingPeriod>85</samplingPeriod>
  <pointerEmulationRate>2</pointerEmulationRate>
  <pointerEmulationDistance>10.0</pointerEmulationDistance>
  <GlobalTarget>
    <GestureRecognizers>
      <LongPress id="1">
        <numTouchesRequired>1</numTouchesRequired>
        <minPressDuration>950</minPressDuration>
        <recognitionThreshold>10.0</recognitionThreshold>
        <abortList>
          <allowSimultaneousRecognition>false</allowSimultaneousRecognition>
        </abortList>
          <XTestLongPressRightClick>
            <RightClickShortcut>
              <button>3</button>
            </RightClickShortcut>
          </XTestLongPressRightClick>
      </LongPress>
      <LongPress id="10">
        <numTouchesRequired>2</numTouchesRequired>
        <minPressDuration>950</minPressDuration>
        <recognitionThreshold>10.0</recognitionThreshold>
        <abortList>
          <allowSimultaneousRecognition>false</allowSimultaneousRecognition>
        </abortList>
          <XTestLongPressMove>
            <MoveShortcut>
              <button modifier1="Alt_L">1</button>
            </MoveShortcut>
          </XTestLongPressMove>
      </LongPress>
      <Tap id="11">
        <numTouchesRequired>2</numTouchesRequired>
        <numTapsRequired>1</numTapsRequired>
        <maxTapDelay>400</maxTapDelay>
        <maxTapDuration>900</maxTapDuration>
        <maxTapDistance>100.0</maxTapDistance>
        <recognitionThreshold>1.0</recognitionThreshold>
        <abortList>
          <allowSimultaneousRecognition>false</allowSimultaneousRecognition>
        </abortList>
        <XTestTapRightClick>
          <RightClickShortcut>
            <key>Menu</key>
          </RightClickShortcut>
        </XTestTapRightClick>
      </Tap>
      <Pan id="2">
        <maxNumTouchesRequired>2</maxNumTouchesRequired>
        <minNumTouchesRequired>2</minNumTouchesRequired>
        <recognitionThreshold>20.0</recognitionThreshold>
        <abortList>
          <allowSimultaneousRecognition>true</allowSimultaneousRecognition>
          <id>4</id>
        </abortList>
        <XTestPanScroll>
          <accumulator>2</accumulator>
          <maxVelocity>25.0</maxVelocity>
          <minVelocity>0.042</minVelocity>
          <ScrollLeftShortcut>
            <key modifier1="Control_L">Left</key>
          </ScrollLeftShortcut>
          <ScrollRightShortcut>
            <key modifier1="Control_L">Right</key>
          </ScrollRightShortcut>
          <ScrollUpShortcut>
            <button>4</button>
          </ScrollUpShortcut>
          <ScrollDownShortcut>
            <button>5</button>
          </ScrollDownShortcut>
        </XTestPanScroll>
      </Pan>
      <Swipe id="3">
        <numTouchesRequired>1</numTouchesRequired>
        <maxDuration>150</maxDuration>
        <minDisplacement>60.0</minDisplacement>
        <maxAngle>40.0</maxAngle>
        <direction>Vertical</direction>
        <recognitionThreshold>10.0</recognitionThreshold>
        <abortList>
          <allowSimultaneousRecognition>false</allowSimultaneousRecognition>
        </abortList>
      </Swipe>
    </GestureRecognizers>
  </GlobalTarget>
  <PublicTarget>
    <name>Firefox</name>
    <GestureRecognizers>
      <TwoTouchPinch id="4">
        <recognitionThreshold>20.0</recognitionThreshold>
        <abortList>
          <allowSimultaneousRecognition>true</allowSimultaneousRecognition>
          <id>2</id>
        </abortList>
        <XTestTwoTouchPinchZoom>
          <accumulator>1</accumulator>
          <maxScale>1.60</maxScale>
          <minScale>1.001</minScale>
          <ZoomInShortcut>
            <button modifier1="Control_L">5</button>
          </ZoomInShortcut>
          <ZoomOutShortcut>
            <button modifier1="Control_L">4</button>
          </ZoomOutShortcut>
        </XTestTwoTouchPinchZoom>
      </TwoTouchPinch>
    </GestureRecognizers>
  </PublicTarget>
  <PrivateTarget>
    <name>Konsole</name>
  </PrivateTarget>
  <PrivateTarget>
    <name>Okular</name>
    <GestureRecognizers>
      <Pan id="1">
        <maxNumTouchesRequired>3</maxNumTouchesRequired>
        <minNumTouchesRequired>3</minNumTouchesRequired>
        <recognitionThreshold>20.0</recognitionThreshold>
        <abortList>
          <allowSimultaneousRecognition>false</allowSimultaneousRecognition>
        </abortList>
        <XTestPanMove>
          <MoveShortcut>
            <button modifier1="Alt_L">1</button>
          </MoveShortcut>
        </XTestPanMove>
      </Pan>
  </GestureRecognizers>
  </PrivateTarget>
</gestemas>
