<?xml version="1.0" encoding="UTF-8"?>
<gestemas>
  <samplingPeriod>120</samplingPeriod>
  <pointerEmulationRate>2</pointerEmulationRate>
  <pointerEmulationDistance>10.0</pointerEmulationDistance>
  <GlobalTarget>
    <GestureRecognizers>
      <Swipe id="1">
        <numTouchesRequired>1</numTouchesRequired>
        <maxDuration>150</maxDuration>
        <minDisplacement>60.0</minDisplacement>
        <maxAngle>40.0</maxAngle>
        <direction>Up</direction>
        <bottomMargin>300.0</bottomMargin>
        <recognitionThreshold>10.0</recognitionThreshold>
        <abortList>
          <allowSimultaneousRecognition>false</allowSimultaneousRecognition>
        </abortList>
        <DBusVirtualKeyboard>
          <command>showFromBottom</command>
        </DBusVirtualKeyboard>
      </Swipe>
      <Swipe id="2">
        <numTouchesRequired>1</numTouchesRequired>
        <maxDuration>150</maxDuration>
        <minDisplacement>60.0</minDisplacement>
        <maxAngle>40.0</maxAngle>
        <direction>Down</direction>
        <bottomMargin>300.0</bottomMargin>
        <recognitionThreshold>10.0</recognitionThreshold>
        <abortList>
          <allowSimultaneousRecognition>false</allowSimultaneousRecognition>
        </abortList>
        <DBusVirtualKeyboard>
          <command>hide</command>
        </DBusVirtualKeyboard>
      </Swipe>
    </GestureRecognizers>
  </GlobalTarget>
</gestemas>
