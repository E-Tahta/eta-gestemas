<?xml version="1.0" encoding="UTF-8"?>
<gestemas>
  <movementThreshold>5.0</movementThreshold>
  <Target>
    <name>root</name>
    <GestureRecognizers>
      <TwoTouchPinch>
        <recognitionThresholdFactor>1.0</recognitionThresholdFactor>
        <allowSimultaneousRecognition>false</allowSimultaneousRecognition>
        <Zoom>
          <accumulator>2</accumulator>
          <maxScale>1.50</maxScale>
          <minScale>1.01</minScale>
        </Zoom>
      </TwoTouchPinch>
    </GestureRecognizers>
  </Target>
</gestemas>
